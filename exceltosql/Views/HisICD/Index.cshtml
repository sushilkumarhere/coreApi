@model IEnumerable<exceltosql.Models.his_icd_diagnosis>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IcdCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LD)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IcdCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LD)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IcdID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IcdID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IcdID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
